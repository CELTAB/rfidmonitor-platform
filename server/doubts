  // CREATE CUSTOM routes
  var customRoute = {
    method:'get',
    route:'/teste/a',
    handler: function(req, callback){
      return callback(null, {message:'response for custom route'});
    }
  };

  var customRoute2 = {
    method:'post',
    route:'/teste/a',
    handler: function(req, callback){
      return callback(null, {message:'response for custom route on post'});
    }
  };
  Rfid.customRoute.push(customRoute);
  Rfid.customRoute.push(customRoute2);
